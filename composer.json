{
    "name": "chars/simple-router",
    "description": "Simple routing management built on top nikic/FastRoute with caching option",
    "type": "library",
    "authors": [
        {
            "name": "Charis",
            "email": "me@chars.me"
        }
    ],
    "require": {
            "php": "^7.0",
            "ext-pcre": "*",
            "nikic/fast-route": "~1.2.0@stable",
            "psr/http-message": "^1.0@stable",
            "psr/cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0@stable",
        "phpunit/php-code-coverage": "^5.0",
        "chars/simple-http": "^v0.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Resilient\\": "src"
        }
    },
    "suggest": {
        "symfony/cache": "PSR16 Cache Interface Implementation for routes caching"
    },
    "minimum-stability": "stable"
}
